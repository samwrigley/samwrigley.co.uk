{
    "name": "samwrigley/samwrigley.co.uk",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "calebporzio/gitdown": "1.3.2",
        "fideloper/proxy": "4.3.0",
        "fruitcake/laravel-cors": "1.0.6",
        "guzzlehttp/guzzle": "6.5.3",
        "jtant/laravel-env-sync": "4.0.0",
        "laravel/framework": "7.11.0",
        "laravel/horizon": "4.3.2",
        "laravel/slack-notification-channel": "2.0.2",
        "laravel/telescope": "3.4.0",
        "laravel/tinker": "2.4.0",
        "nothingworks/blade-svg": "0.3.4",
        "spatie/laravel-csp": "2.5.1",
        "spatie/laravel-feed": "2.6.2",
        "spatie/laravel-honeypot": "2.2.0",
        "spatie/laravel-sitemap": "5.7.0",
        "spatie/laravel-view-models": "1.3.0",
        "spatie/schema-org": "2.13.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.3.3",
        "facade/ignition": "2.0.2",
        "fzaninotto/faker": "1.9.1",
        "laravel/dusk": "5.11.0",
        "mockery/mockery": "1.3.1",
        "nunomaduro/collision": "4.2.0",
        "nunomaduro/larastan": "0.5.8",
        "nunomaduro/phpinsights": "1.14.0",
        "phpunit/phpunit": "9.1.4",
        "squizlabs/php_codesniffer": "3.5.5",
        "symfony/css-selector": "5.0.7",
        "timacdonald/log-fake": "1.6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
