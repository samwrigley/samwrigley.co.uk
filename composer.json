{
    "name": "samwrigley/samwrigley.co.uk",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "bensampo/laravel-enum": "3.0.0",
        "blade-ui-kit/blade-heroicons": "0.2.2",
        "fideloper/proxy": "4.4.0",
        "fruitcake/laravel-cors": "2.0.2",
        "graham-campbell/markdown": "13.1.1",
        "guzzlehttp/guzzle": "7.1.1",
        "laravel/framework": "8.7.1",
        "laravel/horizon": "5.2.0",
        "laravel/legacy-factories": "1.0.5",
        "laravel/slack-notification-channel": "2.2.0",
        "laravel/telescope": "4.0.1",
        "laravel/tinker": "2.4.2",
        "laravel/ui": "3.0.0",
        "ohdearapp/ohdear-php-sdk": "3.0.1",
        "predis/predis": "1.1.6",
        "sentry/sentry-laravel": "2.0.0",
        "spatie/laravel-csp": "2.6.1",
        "spatie/laravel-feed": "3.0.1",
        "spatie/laravel-honeypot": "3.0.0",
        "spatie/laravel-newsletter": "4.8.1",
        "spatie/laravel-responsecache": "6.6.5",
        "spatie/laravel-schedule-monitor": "1.0.4",
        "spatie/laravel-sitemap": "5.8.0",
        "spatie/laravel-view-models": "1.3.1 ",
        "spatie/laravel-webhook-client": "2.7.2",
        "spatie/schema-org": "3.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.5.1",
        "facade/ignition": "2.3.7",
        "fzaninotto/faker": "1.9.1",
        "laracasts/cypress": "1.1.11",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "5.0.2",
        "nunomaduro/larastan": "0.6.4",
        "nunomaduro/phpinsights": "1.14.0",
        "phpunit/phpunit": "9.3.11",
        "squizlabs/php_codesniffer": "3.5.6",
        "symfony/css-selector": "5.1.6",
        "timacdonald/log-fake": "1.7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi"
        ]
    }
}
