{
    "name": "samwrigley/samwrigley.co.uk",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "bensampo/laravel-enum": "3.4.1",
        "blade-ui-kit/blade-heroicons": "1.2.0",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "2.0.4",
        "guzzlehttp/guzzle": "7.3.0",
        "laravel/framework": "8.54.0",
        "laravel/horizon": "5.7.10",
        "laravel/slack-notification-channel": "2.3.1",
        "laravel/telescope": "4.6.1",
        "laravel/tinker": "2.6.1",
        "laravel/ui": "3.3.0",
        "ohdearapp/ohdear-php-sdk": "3.2.1",
        "predis/predis": "1.1.7",
        "sentry/sentry-laravel": "2.8.0",
        "spatie/commonmark-highlighter": "2.1.1",
        "spatie/laravel-csp": "2.6.4",
        "spatie/laravel-feed": "4.0.4",
        "spatie/laravel-honeypot": "4.0.0",
        "spatie/laravel-newsletter": "4.10.0",
        "spatie/laravel-responsecache": "7.1.0",
        "spatie/laravel-schedule-monitor": "2.4.3",
        "spatie/laravel-sitemap": "6.0.4",
        "spatie/laravel-view-models": "1.3.2 ",
        "spatie/laravel-webhook-client": "3.0.1",
        "spatie/schema-org": "3.4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.6.2",
        "brianium/paratest": "6.3.1",
        "facade/ignition": "2.11.3",
        "fakerphp/faker": "1.15.0",
        "mockery/mockery": "1.4.3",
        "nunomaduro/collision": "5.8.0",
        "nunomaduro/larastan": "0.6.10",
        "nunomaduro/phpinsights": "2.0.1",
        "phpunit/phpunit": "9.5.8",
        "spatie/phpunit-snapshot-assertions": "4.2.7",
        "squizlabs/php_codesniffer": "3.6.0",
        "symfony/css-selector": "5.3.4",
        "timacdonald/log-fake": "1.9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi"
        ]
    }
}
